  "bugs": {
    "url": "https://github.com/angelfraga/tydi/issues"
  },
  "homepage": "https://github.com/angelfraga/tydi#readme",
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
